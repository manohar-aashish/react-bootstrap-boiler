{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "emmet.includeLanguages": {
    "ejs": "html",
    "javascript": "javascriptreact"
  },
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/src/static/css"
    }
  ],
  "liveSassCompile.settings.generateMap": false
}
